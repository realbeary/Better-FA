{
  "name": "Better FA (Beta)",
  "description": "An extension that adds a bunch of new features to FurAffinity",
  "version": "0.2",
  "manifest_version": 3,
  "permissions": ["storage","tabs"],
  "host_permissions":["https://*.furaffinity.net/*"],
  "content_scripts":[
    {
        "matches": ["https://www.furaffinity.net/"],
        "js": ["contentscript.js"],
        "css": ["style.css"]
    }
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
      }
  },
  "icons": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
}